{
  "manifest_version": 3,
  "name": "Autofill",
  "version": "1.0",
  "description": "",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
